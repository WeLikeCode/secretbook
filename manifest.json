{
	"name": "Securebook",
	"description": "A secure protocol for communicating with friends over Facebook.",
	"manifest_version": 2,
	"version": "0.11",
	"icons": {
		"16": "images/favicon.gif",
		"128": "images/128.png"
	},
    "content_scripts": [{
        "js": ["js/lib/jquery-1.9.1.js", "js/lib/keymaster.js", "js/inject.js"],
        "matches": ["http://www.facebook.com/*", "https://www.facebook.com/*"]
    }],
    "background": {
    	"page": "background.html"
    },
    "web_accessible_resources": [
    	"index.html", "js/lib/jpg_encoder.js", "js/lib/jpg_decoder.js", "js/lib/seedrandom.js", "js/lib/mlbc.js", "js/lib/dct.js"
    ],
    "permissions": ["tabs", "https://*/", "http://*/"]
}